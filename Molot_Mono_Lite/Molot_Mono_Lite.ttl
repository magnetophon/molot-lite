@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix unit: <http://lv2plug.in/ns/extensions/units#> .
@prefix epp: <http://lv2plug.in/ns/ext/port-props#> .

<urn:magnetophon:Molot_Mono_Lite>
 a lv2:Plugin, lv2:CompressorPlugin ;
 doap:name "Molot_Mono_Lite" ;
 doap:license <https://spdx.org/licenses/GPL-3.0-or-later> ;

 doap:maintainer [
    foaf:name "Bart Brouns" ;
    foaf:homepage <https://magnetophon.nl/> ;
 ] ;

 doap:developer [
    foaf:name "Vladislav Goncharov" ;
    foaf:homepage <https://vladgsound.wordpress.com> ;
 ] ;

 rdfs:comment "Compressor Molot Lite Mono" ;

 lv2:minorVersion 2 ;
 lv2:microVersion 0 ;

 lv2:optionalFeature lv2:hardRTCapable ;

 lv2:requiredFeature <http://lv2plug.in/ns/ext/buf-size#boundedBlockLength> ,
  <http://lv2plug.in/ns/ext/urid#map> ;

 lv2:port [
  a lv2:InputPort, lv2:AudioPort ;
  lv2:index 0 ;
  lv2:name "Audio In 1" ;
  lv2:symbol "AUDIO_IN_1" ;
 ] ,

 [
  a lv2:OutputPort, lv2:AudioPort ;
  lv2:index 1 ;
  lv2:name "Audio Out 1" ;
  lv2:symbol "AUDIO_OUT_1" ;
 ] ,

 [
  a lv2:InputPort, lv2:ControlPort ;
  lv2:index 2 ;
  lv2:name "Input" ;
  lv2:symbol "INPUT" ;
  lv2:default 0 ;
  lv2:minimum -12 ;
  lv2:maximum 12 ;
  unit:unit unit:db ;
 ] ,
 [
  a lv2:InputPort, lv2:ControlPort ;
  lv2:index 3 ;
  lv2:name "Filter" ;
  lv2:symbol "FILTER" ;
  lv2:default 40 ;
  lv2:minimum 40 ;
  lv2:maximum 260 ;
  lv2:scalePoint [ rdfs:label "Off"; rdf:value 40 ];
 ] ,
 [
  a lv2:InputPort, lv2:ControlPort ;
  lv2:index 4 ;
  lv2:name "Threshold" ;
  lv2:symbol "THRESHOLD" ;
  lv2:default -10 ;
  lv2:minimum -40 ;
  lv2:maximum 0 ;
  unit:unit unit:db ;
 ] ,
 [
  a lv2:InputPort, lv2:ControlPort ;
  lv2:index 5 ;
  lv2:name "Knee" ;
  lv2:symbol "KNEE" ;
  lv2:default 1 ;
  lv2:minimum 0 ;
  lv2:maximum 1 ;
  lv2:portProperty lv2:enumeration ;
  lv2:scalePoint [ rdfs:label "Soft"; rdf:value 0 ],
   [ rdfs:label "Hard"; rdf:value 1 ];
 ] ,
 [
  a lv2:InputPort, lv2:ControlPort ;
  lv2:index 6 ;
  lv2:name "Attack" ;
  lv2:symbol "ATTACK" ;
  lv2:default 10 ;
  lv2:minimum 1 ;
  lv2:maximum 100 ;
  unit:unit unit:ms ;
 ] ,
 [
  a lv2:InputPort, lv2:ControlPort ;
  lv2:index 7 ;
  lv2:name "Atk Mode" ;
  lv2:symbol "ATK_MODE" ;
  lv2:default 0 ;
  lv2:minimum 0 ;
  lv2:maximum 1 ;
  lv2:portProperty lv2:enumeration ;
  lv2:scalePoint [ rdfs:label "Sigma"; rdf:value 0 ],
   [ rdfs:label "Alpha"; rdf:value 1 ];
 ] ,
 [
  a lv2:InputPort, lv2:ControlPort ;
  lv2:index 8 ;
  lv2:name "Release" ;
  lv2:symbol "RELEASE" ;
  lv2:default 100 ;
  lv2:minimum 50 ;
  lv2:maximum 1000 ;
  unit:unit unit:ms ;
 ] ,
 [
  a lv2:InputPort, lv2:ControlPort ;
  lv2:index 9 ;
  lv2:name "Ratio" ;
  lv2:symbol "RATIO" ;
  lv2:default 4.1 ;
  lv2:minimum 1.1 ;
  lv2:maximum 11 ;
  lv2:scalePoint [ rdfs:label "inf"; rdf:value 11 ];
 ] ,
 [
  a lv2:InputPort, lv2:ControlPort ;
  lv2:index 10 ;
  lv2:name "Makeup" ;
  lv2:symbol "MAKEUP" ;
  lv2:default 0 ;
  lv2:minimum -12 ;
  lv2:maximum 12 ;
  unit:unit unit:db ;
 ] ,
 [
  a lv2:InputPort, lv2:ControlPort ;
  lv2:index 11 ;
  lv2:name "Dry / Wet" ;
  lv2:symbol "DRY_WET" ;
  lv2:default 0 ;
  lv2:minimum 0 ;
  lv2:maximum 99 ;
  unit:unit unit:pc ;
 ] ,
 [
  a lv2:InputPort, lv2:ControlPort ;
  lv2:index 12 ;
  lv2:name "Oversampling" ;
  lv2:symbol "OVERSAMPLING" ;
  lv2:default 0 ;
  lv2:minimum 0 ;
  lv2:maximum 4 ;
  lv2:portProperty lv2:enumeration ;
  lv2:portProperty epp:hasStrictBounds ;
  lv2:scalePoint [ rdfs:label "None"; rdf:value 0 ],
   [ rdfs:label "2x"; rdf:value 1 ],
   [ rdfs:label "4x"; rdf:value 2 ],
   [ rdfs:label "8x"; rdf:value 3 ],
   [ rdfs:label "16x"; rdf:value 4 ];
 ] ,
 [
  a lv2:OutputPort, lv2:ControlPort ;
  lv2:index 13 ;
  lv2:name "Gain Reduction" ;
  lv2:symbol "GAIN_REDUCTION" ;
  lv2:default 0 ;
  lv2:minimum -40 ;
  lv2:maximum 0 ;
  unit:unit unit:db ;
 ].
